{
  "name": "circleci-orb",
  "version": "0.6.0",
  "description": "See [orb.yml](orb.yml)",
  "main": "index.js",
  "scripts": {
    "test": "npm run unit",
    "orb:namespace": "circleci namespace create cypress-io github cypress-io",
    "orb:validate": "circleci orb validate orb.yml",
    "orb:create": "circleci orb create cypress-io/cypress",
    "orb:list": "circleci orb list cypress-io",
    "preorb:publish": "npm run orb:validate",
    "orb:publish": "node ./scripts/publish",
    "orb:publish:dev": "node ./scripts/publish --dev",
    "next-ver": "next-ver",
    "set-next-version": "next-ver --go",
    "unit": "ava-ts 'test/*.test.ts'",
    "doc:examples": "ts-node ./scripts/examples > examples.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/circleci-orb.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cypress-io/circleci-orb/issues"
  },
  "homepage": "https://github.com/cypress-io/circleci-orb#readme",
  "devDependencies": {
    "@types/common-tags": "1.4.0",
    "@types/node": "10.12.2",
    "ava": "0.25.0",
    "ava-ts": "0.25.2",
    "common-tags": "1.8.0",
    "execa": "1.0.0",
    "husky": "1.1.2",
    "js-yaml": "3.12.0",
    "json2md": "1.6.1",
    "minimist": "1.2.0",
    "next-ver": "1.8.0",
    "temp-write": "3.4.0",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run orb:validate && npm run unit"
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
